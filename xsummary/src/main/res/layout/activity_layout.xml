<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <include
        android:id="@+id/itemLayout"
        layout="@layout/item_main"
        android:layout_width="405dp"
        android:layout_height="87dp"
        tools:layout_editor_absoluteX="6dp"
        tools:layout_editor_absoluteY="5dp" />

    <include
        android:id="@+id/include2"
        layout="@layout/item_other"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <include layout="@layout/item_other1" />

    <include layout="@layout/item_other2" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.5663475" />

    <ViewStub
        android:id="@+id/viewStub2"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout="@layout/item_other3"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />


</androidx.constraintlayout.widget.ConstraintLayout>